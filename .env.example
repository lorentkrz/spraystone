###############################################
# Frontend runtime configuration (Vite)
# NOTE: Do NOT put any real API keys here for production.
# Use the proxy (server) for any secret-bearing calls.
###############################################

# Lead capture: 'before' or 'after'
VITE_LEAD_GATING_MODE=before

# Enable UI mock text when chat backend is not configured
VITE_DEV_MODE=false

# Image provider: 'proxy-openai' (recommended), 'azure-openai', 'openai', 'gemini'
VITE_IMAGE_PROVIDER=proxy-openai

# Text provider: 'none' (mock text), 'azure-openai', 'openai', 'gemini'
VITE_TEXT_PROVIDER=none

# Proxy endpoint (local dev)
VITE_PROXY_IMAGE_ENDPOINT=http://localhost:8787/api/apply-spraystone

# Optional model configs (no secrets)
VITE_OPENAI_CHAT_MODEL=gpt-4o
VITE_OPENAI_IMAGE_MODEL=gpt-image-1.5

# Optional: UI-only estimate for image generation cost/time
# Example: "€0.04", "~€0.08", or "1 credit"
VITE_IMAGE_GENERATION_COST_LABEL=
VITE_IMAGE_GENERATION_ETA_SECONDS=30

# Optional: Gemini (text) if you use it via client (not recommended for prod)
VITE_GEMINI_API_KEY=

# Google Maps API Key (for address autocomplete)
VITE_GOOGLE_MAPS_API_KEY=

# Optional: Azure endpoints (no keys; only URLs)
VITE_AZURE_IMAGE_GENERATIONS_ENDPOINT=
VITE_AZURE_IMAGE_EDITS_ENDPOINT=
VITE_AZURE_CHAT_COMPLETIONS_ENDPOINT=
